{
  "expo": {
    "name": "ParcelPoint",
    "slug": "ParcelPoint",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#122D38"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aldomano.ParcelPoint"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aldomano.ParcelPoint"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8a8b1df8-562e-4a41-8205-267c665d357f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8a8b1df8-562e-4a41-8205-267c665d357f"
    },
    "assetBundlePatterns": ["assets/**/*"]
  }
}
